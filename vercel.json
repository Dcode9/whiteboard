{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {"distDir": "."}
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "JWT_SECRET": "@jwt_secret",
    "REDIRECT_URI": "@redirect_uri"
  }
}
